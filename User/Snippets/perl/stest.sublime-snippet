use Time::HiRes qw( gettimeofday tv_interval );
my \$debug_start = [gettimeofday];
my \$debug_end = [gettimeofday];
warn '[[[TIME]]] ' . tv_interval( \$debug_start, \$debug_end );
]]></content>
